  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "6.16.*",
    "babel-preset-es2015": "6.16.*",
    "babelify": "7.3.*",
    "bitcoin-exchange-client": "v0.2.*"
  },
  "devDependencies": {
    "babel-cli": "~6.24.0",
    "browserify": "13.*",
    "browserify-istanbul": "2.0.*",
    "coveralls": "^2.11.14",
    "eslint": "2.*",
    "git-changelog": "1.0.*",
    "jasmine-core": "^2.2.0",
    "jasmine-es6-promise-matchers": "^2.0.1",
